# Mixpanel 설정
MIXPANEL_SERVICE_ACCOUNT=your_service_account_here
MIXPANEL_SERVICE_PASSWORD=your_service_password_here
MIXPANEL_PROJECT_ID=your_project_id_here

# 웹사이트 설정
BASE_URL=https://your-website.com

# 브라우저 설정
HEADLESS=true
BROWSER_TYPE=chromium

# 감사 설정
TEST_TIMEOUT=30
MAX_RETRIES=3

# 로깅 설정
DEBUG=false
LOG_LEVEL=INFO
